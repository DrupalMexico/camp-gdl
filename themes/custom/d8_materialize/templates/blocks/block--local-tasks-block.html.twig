{% extends "@block/block.html.twig" %}
{#
/**
 * @file
 * Theme override for tabs.
 */
#}
{% block content %}
    {% if content %}
        <div class="tabs-container" aria-label="{{ 'Tabs'|t }}">
            <div class="container">
                {{ content }}
            </div>
        </div>
    {% endif %}
{% endblock %}